{
    "name": "lin3s/wp-phpspec-brigde",
    "description": "PhpSpec bridge for using this BDD fw with WordPress internal functions",
    "keywords": ["lin3s", "wordpress", "phpspec", "bridge"],
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "LIN3S",
            "email": "info@lin3s.com",
            "homepage": "https://lin3s.com"
        }
    ],
    "require": {
        "php": "^5.5|^7.0"
    },
    "require-dev": {
        "fabpot/php-cs-fixer": "^1.11"
    },
    "scripts": {
        "cs": [
            "php-cs-fixer fix --config-file=.php_cs"
        ]
    },
    "autoload": {
        "psr-4": {
            "LIN3S\\WordPressPhpSpecBridge\\": "src/LIN3S/WordPressPhpSpecBridge/"
        }
    },
    "suggest": {
        "phpspec/phpspec": "SpecBDD Framework for PHP"
    }
}
